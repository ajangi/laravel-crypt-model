{
    "name": "ajangi/laravel-crypt-model",
    "description": "Encoding and Decoding laravel model attributes made easy.",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Alireza Jangi",
            "email": "ajangi@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LaravelCryptModel\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "facade/ignition-contracts": "^1.0",
        "illuminate/contracts": "^8.0",
        "ext-json": "*",
        "illuminate/database": "8.x-dev",
        "ext-mcrypt": "*",
        "ext-openssl": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelCryptModel\\LaravelCryptoModelServiceProvider"
            ]
        }
    }
}
